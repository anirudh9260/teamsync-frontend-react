{
    "printWidth": 80,
    "trailingComma": "all",
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "importOrder": [
        "^react(.*)",
        "antd/(.*)",
        "<THIRD_PARTY_MODULES>",
        "@/(.*)",
        "^[./]"
    ],
    "importOrderSeparation": true
}
